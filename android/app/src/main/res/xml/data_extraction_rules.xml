<?xml version="1.0" encoding="utf-8"?>
<!-- Backup rules for API 31+. See https://developer.android.com/about/versions/12/backup-restore#xml-changes -->
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude encrypted auth prefs - keys are in Keystore which isn't backed up -->
        <exclude domain="sharedpref" path="auth_prefs.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude encrypted auth prefs - keys are in Keystore which isn't backed up -->
        <exclude domain="sharedpref" path="auth_prefs.xml"/>
    </device-transfer>
</data-extraction-rules>
